{
  "name": "media-app",
  "version": "1.0.0",
  "description": "Web application for microflora information and clinical multimodal data",
  "repository": "https://github.com/MeDIA-team/MeDIA",
  "author": "MeDIA-team",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "lint": "eslint --ext .ts,.js,.vue .",
    "exec-ts-node": "ts-node -r tsconfig-paths/register --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" --files",
    "config:validate": "yarn exec-ts-node ./command/configValidate.ts",
    "config:dumpSchemas": "yarn exec-ts-node ./command/configDumpSchemas.ts",
    "bulk": "yarn exec-ts-node ./command/esBulk.ts",
    "bulk:validate": "yarn exec-ts-node ./command/esBulkValidate.ts",
    "test": "jest --silent",
    "test:generateTestEntries": "yarn exec-ts-node ./tests/generateTestEntries.ts",
    "test:bulk": "yarn test:bulk:data && yarn test:bulk:patient && yarn test:bulk:sample",
    "test:bulk:data": "yarn bulk data ./tests/config.test.json ./tests/data.test.json",
    "test:bulk:patient": "yarn bulk patient ./tests/config.test.json ./tests/patient.test.json",
    "test:bulk:sample": "yarn bulk sample ./tests/config.test.json ./tests/sample.test.json"
  },
  "dependencies": {
    "nuxt": "^2.15.6"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "@mdi/font": "^5.9.55",
    "@nuxt/types": "^2.15.6",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/vuetify": "^1.11.3",
    "@types/jest": "^26.0.23",
    "@vue/test-utils": "^1.2.1",
    "chart.js": "~2",
    "dayjs": "^1.10.5",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "nuxt-basic-auth-module": "^1.4.0",
    "nuxt-clipboard2": "^0.2.1",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "vue-chartjs": "^3.5.1",
    "vue-jest": "^3.0.7"
  }
}
