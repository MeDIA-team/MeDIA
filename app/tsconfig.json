{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "allowJs": true,
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./@nuxt/types",
      "./src/types",
      "./node_modules/vuetify/types"
    ],
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
